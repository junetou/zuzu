<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" 
		"tiles-config_3_0.dtd">
<tiles-definitions>
	
	<definition name="tiles/index/*" template="/WEB-INF/index/template.jsp" >
	    <put-attribute name="content" value="/WEB-INF/index/{1}.jsp" />
	</definition>
	
	<definition name="tiles/own/*" template="/WEB-INF/own/template.jsp" >
	    <put-attribute name="content" value="/WEB-INF/own/{1}.jsp" />
	</definition>
	
	<definition name="tiles/form/*" template="/WEB-INF/form/template.jsp" >
	    <put-attribute name="content" value="/WEB-INF/form/{1}.jsp" />
	</definition>
	
	<definition name="tiles/particular/*" template="/WEB-INF/particular/template.jsp" >
	    <put-attribute name="content" value="/WEB-INF/particular/{1}.jsp" />
	</definition>
	
</tiles-definitions>